{
  "name": "scp-remote-action",
  "version": "0.0.1",
  "private": true,
  "description": "scp-remote-action,copy local archive file to remote server node by ssh chanel",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/*.{ts,tsx}\"",
    "format-check": "prettier --check \"src/*.{ts,tsx}\"",
    "package": "ncc build --source-map",
    "lint": "eslint src  --ext .ts",
    "all": "npm run lint && npm run build && npm run format && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lengxing-lx/scp-remote-action.git"
  },
  "keywords": [
    "actions",
    "ssh",
    "remote"
  ],
  "author": "wenchui",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/http-client": "^1.0.6",
    "@actions/io": "^1.0.2",
    "@actions/tool-cache": "^1.5.5",
    "@types/fs-extra": "^9.0.13",
    "fs-extra": "^10.0.1",
    "semver": "7.3.5"
  },
  "devDependencies": {
    "@cloud/eslint-config-cbc": "^1.7.2",
    "@types/node": "^16.11.25",
    "@types/semver": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "@zeit/ncc": "^0.21.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-bevry": "^3.27.0",
    "eslint-plugin-mocha": "^9.0.0",
    "jest": "^27.2.5",
    "jest-circus": "^27.2.5",
    "nock": "^10.0.6",
    "prettier": "^1.17.1",
    "ts-jest": "^27.0.5",
    "typescript": "^4.5.0"
  }
}
